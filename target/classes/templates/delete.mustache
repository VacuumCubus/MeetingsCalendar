<html>

<body>
<div>
    <form method="post">
        <input type="text" name="id" placeholder="ID of the meeting to be deleted" />
        <button type="submit">Delete</button>
    </form>
</div>
<style>
table {
    border-collapse: collapse;
}

table, td, th {
    border: 1px solid #999;
    padding: 5px;
}
</style>
</head>
<h2>Meetings</h2>
 <table>
     <tr>
         <th>ID</th>
         <th>Meeting name</th>
         <th>Description</th>
         <th>FULL NAME</th>
         <th>Meeting date</th>
         <th>Meeting timr</th>
         <th>Time duration</th>
     </tr>
{{#meetings}}
    <tr>
        <td>{{id}}</td>
        <td>{{meeting_name}}</td>
        <td>{{description}}</td>
        <td>{{full_name}}</td>
        <td>{{date}}</td>
        <td>{{time}}</td>
        <td>{{time_duration}}</td>
    </tr>
{{/meetings}}
 </table>
</body>
</html>